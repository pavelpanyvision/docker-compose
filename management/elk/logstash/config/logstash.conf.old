input {
	redis {
		host => redis
		port => 6379
		key => "logstash"
		data_type => "list"
		codec => json
	}
}

filter {
	grok {
		match => { "message" => "%{COMBINEDAPACHELOG}" }
	}
	date {
		match => [ "timestamp" , "YYYY-MM-dd HH:mm:ss,SSS" ]
	}
}

output {
	elasticsearch {
		hosts => ["elasticsearch:9200"]
	}
}

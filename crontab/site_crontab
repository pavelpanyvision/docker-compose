30 3 * * * track_delete=$(date --iso-8601=seconds -d "$TO_DATE_TRACK_DELETE day ago") && curl -G -X "DELETE" --max-time 82800 --data-urlencode "toDate=$track_delete" http://${API_IP}:3000/without-subject
